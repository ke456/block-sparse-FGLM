
function get_equations()
    M<x,y,z>:=PolynomialRing(Integers(),3);

    eqs := [
	2*x*y^4*z^2 + x^3*y^2*z - x^2*y^3*z + 2*x*y*z^2 + 7*y^3 + 7,
	2*x^2*y^4*z + x^2*y*z^2 - x*y^2*z^2 + 2*x^2*y*z - 12*x + 12*y,
	2*y^5*z + x^2*y^2*z - x*y^3*z - x*y^3 + y^4 + 2*y^2*z,
	3*x*y^4*z^3 + x^2*y^2*z - x*y^3*z + 4*y^3*z^2 + 3*x*y*z^3 + 4*z^2 -x + y
	];

    return eqs, "EArnold_2";

end function;
