
function get_equations()
    K<x,y>:=PolynomialRing(Integers(),2);
    a:=1;
    b:=2;

    eqs := [
	2*y^2*(y^2+x^2)+(b^2-3*a^2)*y^2-2*b*y^2*(x+y)+2*a^2*b*(y+x)-a^2*x^2+a^2*(a^2-b^2),
	4*y^3+4*y*(y^2+x^2)-2*b*y^2-4*b*y*(y+x)+2*(b^2-3*a^2)*y+2*a^b,
	4*x*y^2
	];
    return eqs, "bronstein2";
end function;
