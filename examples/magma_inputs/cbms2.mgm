
function get_equations()
    M:=PolynomialRing(Integers(),4);
    x:=[M.i : i in [1..Rank(M)]];
    sys:=[
	x[1]^3 - 3*x[1]^2*x[2] + 3*x[1]*x[2]^2 - x[2]^3 - x[3]^2,
	x[3]^3 - 3*x[3]^2*x[1] + 3*x[3]*x[1]^2 - x[1]^3 - x[2]^2,
	x[2]^3 - 3*x[2]^2*x[3] + 3*x[2]*x[3]^2 - x[3]^3 - x[1]^2
	];
    return sys, "cbms2";
end function;
